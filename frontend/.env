# API Configuration
REACT_APP_API_URL=/api

# WebSocket configuration for development
WDS_SOCKET_PORT=0  # Disable WebSocket for single-port setup

# Environment
NODE_ENV=development

# Frontend configuration
REACT_APP_NAME="FRA Atlas & DSS"
REACT_APP_VERSION=1.0.0

# Disable React's default service worker
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=true

# Development settings
FAST_REFRESH=true
DISABLE_ESLINT_PLUGIN=true

# Backend URL
REACT_APP_BACKEND_URL=https://fra-map-system.preview.emergentagent.com